{
  "name": "anneleenvernaillen.com",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch ../../dist/index.js -x \"cross-env DEBUG=vite-plugin-windicss:options vite\"",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "preview": "vite preview",
    "serve": "pnpm run build && serve dist",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:local": "start-server-and-test serve http://localhost:3000 cypress:run --config baseUrl=http://localhost:3000",
    "typecheck": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint . --fix",
    "fulltest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run typecheck && pnpm run coverage && pnpm run cypress:local"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "animate.css": "^4.1.1",
    "nprogress": "^0.2.0",
    "prism-theme-vars": "^0.2.4",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.29.2",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@types/jest": "^29.2.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.9",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "3.2.0",
    "@vitest/coverage-c8": "^0.24.4",
    "@vue/test-utils": "^2.2.1",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.4.0",
    "@vueuse/head": "^0.9.8",
    "cross-env": "^7.0.3",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-container": "^3.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "nodemon": "^2.0.20",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-vue-components": "^0.22.9",
    "vite": "3.2.2",
    "vite-plugin-pages": "^0.27.1",
    "vite-plugin-pages-sitemap": "^1.4.0",
    "vite-plugin-vue-markdown": "^0.22.1",
    "vite-plugin-windicss": "^1.8.8",
    "vite-ssg": "^0.21.2",
    "vitest": "^0.24.4",
    "vue-tsc": "^1.0.9",
    "windicss": "^3.5.6"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
