{
  "name": "anneleenvernaillen.com",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch ../../dist/index.js -x \"cross-env DEBUG=vite-plugin-windicss:options vite\"",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint . --fix",
    "prepushtest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run build && serve dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "animate.css": "^4.1.1",
    "nprogress": "^0.2.0",
    "prism-theme-vars": "^0.2.4",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.7.18",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.2.0",
    "@vueuse/head": "^0.7.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-container": "^3.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.3",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.2",
    "vite-plugin-pages": "^0.26.0",
    "vite-plugin-pages-sitemap": "^1.3.0",
    "vite-plugin-vue-markdown": "^0.22.1",
    "vite-plugin-windicss": "^1.8.8",
    "vite-ssg": "^0.20.2",
